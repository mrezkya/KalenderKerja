generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        Int         @id @default(autoincrement())
  name      String
  email     String      @unique
  kelompok  Int
  Password  String
  Pekerjaan Pekerjaan[]
}

model Pekerjaan {
  id          Int    @id @default(autoincrement())
  j_pekerjaan String
  waktu       String
  UserId      Int
  user        User   @relation(fields: [UserId], references: [id])
  p_jawab     String @default("")
}
